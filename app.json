{
  "name": "nit-attendance-bot",
  "description": "NIT Attendance Bot",
  "repository": "https://github.com/JameelKaisar/NIT-Attendance-Bot",
  "stack": "heroku-22",
  "buildpacks": [
    {
      "url": "heroku/python"
    },
    {
      "url": "heroku-community/apt"
    }
  ],
  "env": {
    "API_ID": {
      "description": "Get API_ID from https://my.telegram.org"
    },
    "API_HASH": {
      "description": "Get API_Hash from https://my.telegram.org"
    },
    "BOT_TOKEN": {
      "description": "Get BOT_TOKEN from https://t.me/BotFather"
    },
    "TESSDATA_PREFIX": {
      "description": "Run 'find -iname tessdata' in Console!",
      "value": "./.apt/usr/share/tesseract-ocr/4.00/tessdata"
    }
  }
}
